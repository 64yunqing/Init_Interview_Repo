{
    "compilerOptions": {
        "allowJs": true, // 允许使用JavaScript文件
        "checkJs": true, // 可选：开启JavaScript文件的类型检查
        "outDir": "./dist",
        "module": "commonjs",
        "target": "es6",
        "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
